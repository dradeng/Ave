    "cjs": "0.0.11",
    "concurrently": "^3.5.1",
    "crypto": "^1.0.1",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "gravatar": "^1.6.0",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.2.0",
    "mapbox-gl": "^0.49.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.0.12",
    "multer": "^1.4.0",
    "multer-gridfs-storage": "^3.2.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
